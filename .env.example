# ===========================================
# EDUECOSYSTEM BACKEND - ENVIRONMENT TEMPLATE
# ===========================================
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# ----- ENVIRONMENT -----
ENVIRONMENT=production
VERSION=2.0.0
LOG_LEVEL=WARNING
DEBUG=false

# ----- SECURITY -----
# IMPORTANT: Generate a new key with: python -c "import secrets; print(secrets.token_urlsafe(32))"
SECRET_KEY=your-generated-secret-key-here

# ----- DATABASE -----
# For production, use PostgreSQL
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@YOUR_RDS_ENDPOINT:5432/eduecosystem_prod

# For development, leave blank (uses SQLite)
# DATABASE_URL=

# ----- SUPERUSER -----
FIRST_SUPERUSER=ktej255@gmail.com
FIRST_SUPERUSER_PASSWORD=Tej@1106

# ----- CORS -----
# Comma-separated list of allowed origins
BACKEND_CORS_ORIGINS=https://eduecosystem-frontend.vercel.app,http://localhost:3001

# ----- AI CONFIGURATION -----
# Get keys from https://openrouter.ai/keys
FREE_GEMINI_API_KEY=your-free-openrouter-key
PAID_GEMINI_API_KEY=your-paid-openrouter-key
GEMMA_API_KEY=your-gemma-key
LLAMA_API_KEY=your-llama-key
DEFAULT_AI_MODEL=google/gemini-3-flash-preview

# ----- EMAIL (Optional) -----
MAIL_USERNAME=your_email@gmail.com
MAIL_PASSWORD=your_app_password
MAIL_FROM=your_email@gmail.com
MAIL_SERVER=smtp.gmail.com
MAIL_PORT=587
MAIL_SUPPRESS_SEND=0

# ----- AWS S3 (Optional) -----
STORAGE_BACKEND=local
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=
AWS_REGION=us-east-1

# ----- STRIPE (Optional) -----
STRIPE_SECRET_KEY=sk_test_placeholder
STRIPE_PUBLISHABLE_KEY=pk_test_placeholder
STRIPE_WEBHOOK_SECRET=whsec_placeholder
